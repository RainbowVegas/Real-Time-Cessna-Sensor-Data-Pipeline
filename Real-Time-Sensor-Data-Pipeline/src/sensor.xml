<?xml version="1.0"?>
<PropertyList>
<generic>
   <output>
      <line_separator>newline</line_separator>
      <var_separator>tab</var_separator>
   
      <chunk>
         <name>temperature-F</name>
         <node>/environment/temperature-degf</node>
         <type>double</type>
         <format>TF=%.6lf</format>
      </chunk>

      <chunk>
         <name>altitude</name>
         <node>/position/altitude-ft</node>
         <type>double</type>
         <format>A=%.6lf</format>
      </chunk>

      <chunk>
         <name>altitude-agl</name>
         <node>/position/altitude-agl-ft</node>
         <type>double</type>
         <format>AGL=%.6lf</format>
      </chunk>

      <chunk>
        <name>speed</name>
        <format>V=%.6lf</format>
        <node>/velocities/airspeed-kt</node>
        <type>double</type>
      </chunk>

      <chunk>
         <name>vertical-speed-fps</name>
         <node>/velocities/vertical-speed-fps</node>
         <type>double</type>
         <format>VSF=%.6lf</format>
      </chunk>

      <chunk>
         <name>engine-rpm</name>
         <node>/engines/engine/rpm</node>
         <type>double</type>
         <format>ER=%.6lf</format>
      </chunk>

      <chunk>
         <name>throttle</name>
         <node>/controls/engines/engine/throttle</node>
         <type>double</type>
         <format>T=%.6lf</format>
      </chunk>

      <chunk>
         <name>oil-pressure-psi</name>
         <node>/engines/engine/oil-pressure-psi</node>
         <type>double</type>
         <format>OP=%.6lf</format>
      </chunk>

      <chunk>
         <name>oil-temperature-F</name>
         <node>/engines/engine/oil-temperature-degf</node>
         <type>double</type>
         <format>OT=%.6lf</format>
      </chunk>

      <chunk>
         <name>fuel-cap-gph</name>
         <node>/consumables/fuel/total-fuel-gal_us</node>
         <type>double</type>
         <format>FCG=%.6lf</format>
      </chunk>

      <chunk>
         <name>fuel-flow-gph</name>
         <node>/engines/engine/fuel-flow-gph</node>
         <type>double</type>
         <format>FFG=%.6lf</format>
      </chunk>

      <chunk>
        <name>pitch angle (deg)</name>
        <node>/orientation/pitch-deg</node>
        <type>double</type>
        <format>PA=%.6lf</format>
      </chunk>

      <chunk>
        <name>pitch angle rate (degps)</name>
        <node>/orientation/pitch-rate-degps</node>
        <type>double</type>
        <format>PR=%.6lf</format>
      </chunk>

      <chunk>
        <name>roll angle (deg)</name>
        <node>/orientation/roll-deg</node>
        <type>double</type>
        <format>RA=%.6lf</format>
      </chunk>

      <chunk>
        <name>roll angle rate (degps)</name>
        <node>/orientation/roll-rate-degps</node>
        <type>double</type>
        <format>RR=%.6lf</format>
      </chunk>

      <chunk>
        <name>yaw angle (deg)</name>
        <node>/orientation/heading-deg</node>
        <type>double</type>
        <format>YA=%.6lf</format>
      </chunk>

      <chunk>
        <name>yaw angle rate (degps)</name>
        <node>/orientation/yaw-rate-degps</node>
        <type>double</type>
        <format>YR=%.6lf</format>
      </chunk>
   </output>
</generic>
</PropertyList>